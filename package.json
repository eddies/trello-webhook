    "node"
  ],
  "author": "Edwin Shin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eddies/trello-webhook/issues"
  },
  "homepage": "https://github.com/eddies/trello-webhook#readme",
  "dependencies": {},
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "smee-client": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
